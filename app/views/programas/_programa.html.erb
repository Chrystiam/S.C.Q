<tr id=<%= "programa_#{programa.id}" %>>
  <td><%= link_to programa.id, edit_programa_path(programa), :remote => true %></td>
  <td><%= programa.descripcion %></td>
  <td><%= programa.abreviatura %></td>
  <td><%= programa.ficha %></td>
  <td>
    <%= link_to 'Mostrar', programa, :remote => true, :class => 'btn btn-default' %>
    <%= link_to 'Editar', edit_programa_path(programa), :remote => true,
        :class => 'btn btn-warning' %>
    <%= link_to 'Eliminar', programa, 
        :data => { :confirm => 'Esta Seguro?' },
        :method => :delete, :remote => true, :class => 'btn btn-default btn-danger' %>
  </td>
</tr>
