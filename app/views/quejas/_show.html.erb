<%- model_class = Queja -%>
	<br><br>
	<h1><%=t '.title', :default => model_class.model_name.human %></h1>
	<br>

	<div class='queja'>
		<p>id: <%= @queja.id %></p>
		<p>fecha: <%= @queja.fechainforme %></p>
		<p>nombres: <%= @queja.nombres %></p>
		<p>apellidos: <%= @queja.apellidos %></p>
		<p>tipo documento: <%= @queja.tipo_documento.descripcion %></p>
        <p>numero: <%= @queja.identificacion %></p>
		<p>programa: <%= @queja.programa.abreviatura %></p>
		<p>descripcion falta: <%= @queja.descripcion %></p>
		<p>falta: <%= @queja.falta.descripcion %></p>
		<p>testigos: <%= @queja.testigos %></p>
		<p>nombres informante: <%= @queja.nombresinformante %></p>
		<p>apellidos informante: <%= @queja.apellidosinformante %></p>
        <p>direccion informante: <%= @queja.direccioninformante %></p>
		<p>cargo: <%= @queja.cargo.descripcion%></p>
		<p>descargos: <%= @queja.descargos %></p>
		<p>Evidencia: <strong><%= image_tag @queja.evidencia.url(:small) %></strong></p>
	</div>


